{
  "name": "obsidian-link-plugin",
  "version": "1.0.0",
  "description": "Daily note directory management, Future notes, and an introduction to graymatter and templates",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "npx tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "organize-docs": "node scripts/organize-docs.js",
    "docs:validate": "node scripts/validate-docs.js",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "linking",
    "journal",
    "shortcode",
    "emmet"
  ],
  "author": "JKJRDev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jkjrdev/obsidian-link-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/jkjrdev/obsidian-link-plugin/issues"
  },
  "homepage": "https://github.com/jkjrdev/obsidian-link-plugin#readme",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}