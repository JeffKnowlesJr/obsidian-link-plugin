{
  "name": "obsidian-ai-assistant",
  "version": "1.0.0",
  "description": "A powerful plugin for Obsidian that provides direct access to various AI assistants",
  "main": "main.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "ai",
    "claude",
    "chatgpt",
    "assistant"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.10.2",
    "crypto-js": "^4.2.0",
    "openai": "^4.79.1",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/crypto-js": "^4.2.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^16.11.6",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "jest": "^29.7.0",
    "obsidian": "latest",
    "ts-jest": "^29.1.2",
    "typescript": "^5.6.3"
  }
}
