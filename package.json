{
  "name": "obsidian-link-plugin",
  "version": "2.3.0",
  "description": "A plugin for managing links in Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -p tsconfig.prod.json -noEmit && node esbuild.config.mjs production",
